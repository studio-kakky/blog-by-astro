{
  "name": "@example/basics",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev --port 3200",
    "start": "astro dev --port 3200",
    "build": "astro build",
    "preview": "astro preview",
    "prepare": "husky install",
    "gql-codegen": "run-s gql-codegen:*",
    "gql-codegen:load-schema": "source .env && source .env && npx get-graphql-schema -h \"authorization=Bearer $PUBLIC_CONTENTFUL_API_TOKEN\" https://graphql.contentful.com/content/v1/spaces/$PUBLIC_CONTENTFUL_SPACE_ID > src/graphql/schema.graphql",
    "gql-codegen:generate": "graphql-codegen --config ./codegen.yaml",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.11.6",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-graphql-request": "^4.5.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "astro": "^1.0.0-beta.61",
    "eslint": "^8.21.0",
    "eslint-plugin-react": "^7.30.1",
    "get-graphql-schema": "^2.1.2",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "pretty-quick": "^3.1.3",
    "sass": "^1.54.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@astrojs/preact": "^0.5.2",
    "@types/luxon": "^3.0.0",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "luxon": "^3.0.1",
    "preact": "^10.10.0"
  }
}
